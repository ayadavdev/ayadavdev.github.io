{
  "_id": "costco-savings-2024-June",
  "startUrl": [
    "https://web.archive.org/web/20240601152949/https://www.costco.com/online-offers.html"
  ],
  "selectors": [
    {
      "id": "product-name",
      "multiple": false,
      "parentSelectors": [
        "wrapper_for_products"
      ],
      "regex": "",
      "selector": "div.eco-sl1",
      "type": "SelectorText"
    },
    {
      "id": "product-description-optional",
      "multiple": false,
      "parentSelectors": [
        "wrapper_for_products"
      ],
      "regex": "",
      "selector": "div.eco-sl2",
      "type": "SelectorText"
    },
    {
      "id": "wrapper_for_products",
      "multiple": true,
      "parentSelectors": [
        "_root"
      ],
      "selector": "li.eco-coupons:nth-of-type(n+2) a",
      "type": "SelectorElement"
    },
    {
      "id": "product-code",
      "multiple": false,
      "parentSelectors": [
        "wrapper_for_products"
      ],
      "regex": "\\w+$",
      "selector": "div.eco-items",
      "type": "SelectorText"
    },
    {
      "id": "discount-amount",
      "multiple": false,
      "parentSelectors": [
        "wrapper_for_products"
      ],
      "regex": "",
      "selector": "span.eco-dollar:nth-of-type(2)",
      "type": "SelectorText"
    },
    {
      "id": "list-price-before-discount",
      "multiple": false,
      "parentSelectors": [
        "wrapper_for_products"
      ],
      "regex": "",
      "selector": "tr:contains('Price') span.eco-priceTable",
      "type": "SelectorText"
    },
    {
      "id": "availability",
      "multiple": false,
      "parentSelectors": [
        "wrapper_for_products"
      ],
      "regex": "",
      "selector": "div.eco-header",
      "type": "SelectorText"
    },
    {
      "id": "product-image",
      "multiple": false,
      "parentSelectors": [
        "wrapper_for_products"
      ],
      "selector": "img",
      "type": "SelectorImage"
    },
    {
      "id": "offer-validity",
      "multiple": false,
      "parentSelectors": [
        "_root"
      ],
      "regex": "",
      "selector": "p.eco-webValid, p.eco-webvalid-header, #eco-webCoupons .col-xs-12 p",
      "type": "SelectorText"
    }
  ]
}
